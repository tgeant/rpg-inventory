{
  "name": "rpginventory",
  "version": "0.0.0",
  "main": "src",
  "private": true,
  "scripts": {
    "start": "node .",
    "dev": "nodemon -i \"*.test.js\" .",
    "prod": "cross-env NODE_ENV=production nodemon -i \"*.test.js\" -r dotenv-safe/config .",
    "docs": "apidoc -i src -o doc/apidoc && apidoc-markdown -p doc/apidoc -o DOCS.md",
    "postdocs": "open-cli doc/apidoc/index.html",
    "swagger": "apidoc-openapi-3 -i src"
  },
  "devDependencies": {
    "apidoc": "^0.53.0",
    "apidoc-markdown": "^7.2.1",
    "apidoc-openapi-3": "^1.0.2",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "dotenv-safe": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.0",
    "eslint-plugin-standard": "^4.0.1",
    "mongodb-memory-server": "^6.3.3",
    "nock": "^12.0.2",
    "nodemon": "^2.0.2",
    "open-cli": "^7.1.0",
    "sinon": "^4.0.1",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-export-default-from": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@babel/runtime": "^7.9.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "bodymen": "^1.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.9.4",
    "mongoose-id-validator": "^0.6.0",
    "mongoose-keywords": "^0.4.0",
    "morgan": "^1.9.1",
    "querymen": "^2.1.4",
    "request": "^2.88.2",
    "request-promise": "^4.2.5"
  }
}
